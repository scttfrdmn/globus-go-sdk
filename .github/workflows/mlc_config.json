{
  "ignorePatterns": [
    {
      "pattern": "^https://docs.globus.org/api/"
    },
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}